<view class="page">
    <view class="page__bd">
       
        <!--//歌单名字和按钮-->
        <view class="weui-cells weui-cells_after-title">
            <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:100px; height: 100px;"></image>
                </view>
                <view class="weui-cell__bd">歌单名字</view>
                <view class="weui-cell__hd">
                    <image src="../icon/分享.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
            </navigator>
        </view>
        

        <!--歌曲列表-->
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">歌曲1</view>
                <view class="weui-cell__hd">
                    <image src="../icon/暂停.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                    <button style="display:inline-block;" class="weui-btn" type="default" bindtap="openToast">
                        <image src="../icon/删除.jpg" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>      
                    </button>  
                </view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd">歌曲2</view>
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd">歌曲3</view>
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd">歌曲3</view>
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd">歌曲3</view>
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd">歌曲3</view>
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd">歌曲3</view>
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
            </view>
        </view>
      
    </view>
    <view class="weui-tab" >
        <view class="weui-navbar">
                <block wx:for="{{tabs}}" wx:key="*this">
                    <view id="{{index}}" class="weui-navbar__item {{activeIndex == index ? 'weui-bar__item_on' : ''}}" bindtap="tabClick">
                        <view class="weui-navbar__title">{{item}}</view>
                    </view>
                </block>
                <view class="weui-navbar__slider" style="left: {{sliderLeft}}px; transform: translateX({{sliderOffset}}px); -webkit-transform: translateX({{sliderOffset}}px);"></view>
        </view>
            <!--
            <view class="weui-tab__panel">
                <view class="weui-tab__content" hidden="{{activeIndex != 0}}">选项一的内容</view>
                <view class="weui-tab__content" hidden="{{activeIndex != 1}}">选项二的内容</view>
                <view class="weui-tab__content" hidden="{{activeIndex != 2}}">选项三的内容</view>
            </view>
